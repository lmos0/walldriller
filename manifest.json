{
  "manifest_version": 3,
  "name": "Furadeira de Paywalls",
  "version": "1.0",
  "description": "Carrega uma versão da página arquivada no https://archive.ph/",
  "permissions": [
    "tabs",
    "clipboardWrite",
    "notifications"
  ],
  "background": {
    "service_worker": "submit.js" 
  },
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
